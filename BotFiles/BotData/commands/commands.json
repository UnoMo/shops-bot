    {
      "name": "start-giveaway",
      "actions": [
        {
          "name": "Check Permissions",
          "category": "User Action",
          "type": "Check User Permissions",
          "trueActions": [
            {
              "name": "saveGiveAway",
              "category": "Variable",
              "type": "Store Value in Variable",
              "varname": "channelGiveaway",
              "vartype": "Text",
              "savevartype": "server",
              "param": "0"
            },
            {
              "name": "channel",
              "category": "Variable",
              "type": "Store Value in Variable",
              "varname": "channelname",
              "vartype": "Text",
              "savevartype": "temp",
              "param": "1"
            },
            {
              "name": "checkChannel",
              "category": "Variable",
              "type": "Check Variable Value",
              "trueActions": [
                {
                  "name": "checkDuration",
                  "category": "Variable",
                  "type": "Check Variable Value",
                  "trueActions": [
                    {
                      "name": "checkWinners",
                      "category": "Variable",
                      "type": "Check Variable Value",
                      "trueActions": [
                        {
                          "name": "checkGiveaway",
                          "category": "Variable",
                          "type": "Check Variable Value",
                          "trueActions": [
                            {
                              "name": "StartGiveaway",
                              "category": "Message",
                              "type": "Send Embed",
                              "fields": [
                                {
                                  "name": "Duration",
                                  "value": "<${tempVars.duration}>",
                                  "inline": "true"
                                },
                                {
                                  "name": "Prizes",
                                  "value": "<${tempVars.giveawayItem}>",
                                  "inline": "true"
                                },
                                {
                                  "name": "No. of Winners",
                                  "value": "<${tempVars.winners}> Lucky ones",
                                  "inline": "true"
                                }
                              ],
                              "channelname": "${dbsVars.CommandChannel.id}",
                              "color": "3BD4FF",
                              "title": "ðŸŽ‰ðŸŽ‰ Giveaway ðŸŽ‰ðŸŽ‰",
                              "url": "",
                              "authorname": "",
                              "authorimageurl": "",
                              "authorlink": "",
                              "description": "We have started a giveway!  React with ðŸŽ‰ to participate.",
                              "thumbnail": "",
                              "image": "",
                              "footer": "",
                              "timestamp": "false",
                              "savetovariable": "",
                              "savetovariabletype": "temp"
                            }
                          ],
                          "falseActions": [
                            {
                              "name": "No Giveaway",
                              "category": "Message",
                              "type": "Send Message",
                              "channelname": "${dbsVars.CommandAuthor.id}",
                              "messagetext": "You have to specify a valid prize!",
                              "savetovariable": "",
                              "savetovariabletype": "temp"
                            }
                          ],
                          "varname": "${serverVars.channelGiveaway[4+]}",
                          "savevartype": "temp",
                          "compare": "greater than",
                          "value": "NULL"
                        }
                      ],
                      "falseActions": [
                        {
                          "name": "No Winners",
                          "category": "Message",
                          "type": "Send Message",
                          "channelname": "${dbsVars.CommandAuthor.id}",
                          "messagetext": "You have to specify a valid number of winners!",
                          "savetovariable": "",
                          "savetovariabletype": "temp"
                        }
                      ],
                      "varname": "${serverVars.channelGiveaway[3]}",
                      "savevartype": "temp",
                      "compare": "greater than",
                      "value": "0"
                    }
                  ],
                  "falseActions": [
                    {
                      "name": "No Duration",
                      "category": "Message",
                      "type": "Send Message",
                      "channelname": "${dbsVars.CommandAuthor.id}",
                      "messagetext": "You have to specify a valid duration!",
                      "savetovariable": "",
                      "savetovariabletype": "temp"
                    }
                  ],
                  "varname": "${serverVars.channelGiveaway[2]}",
                  "savevartype": "temp",
                  "compare": "greater than",
                  "value": "NULL"
                }
              ],
              "falseActions": [
                {
                  "name": "noChannel",
                  "category": "Message",
                  "type": "Send Message",
                  "channelname": "${dbsVars.CommandAuthor.id}",
                  "messagetext": "You have to mention a valid channel. ",
                  "savetovariable": "",
                  "savetovariabletype": "temp"
                }
              ],
              "varname": "${serverVars.channelGiveaway[1]}",
              "savevartype": "temp",
              "compare": "equal to",
              "value": "${dbsVars.CommandChannel.id}"
            }
          ],
          "falseActions": [
            {
              "name": "No permissions",
              "category": "Message",
              "type": "Send Message",
              "channelname": "${dbsVars.CommandAuthor.id}",
              "messagetext": "You need to have the manage messages permissions to start giveaways.",
              "savetovariable": "",
              "savetovariabletype": "temp"
            }
          ],
          "user": "${dbsVars.CommandAuthor.id}",
          "permissions": [
            "MANAGE_MESSAGES"
          ]
        }
