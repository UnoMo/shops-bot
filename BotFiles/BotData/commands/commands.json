{
  "command": [
    {
      "name": "help",
      "actions": [
        {
          "name": "help menu",
          "category": "Message",
          "type": "Send Embed",
          "fields": [
            {
              "name": "!help",
              "value": "Shows this menu",
              "inline": "true"
            },
            {
              "name": "!info",
              "value": "Shows some info",
              "inline": "true"
            }
          ],
          "channelname": "${dbsVars.CommandChannel.id}",
          "color": "1FFF57",
          "title": "Help Menu",
          "url": "https://unomo.no",
          "authorname": "Bot Name Here",
          "authorimageurl": "",
          "authorlink": "",
          "description": "This is the embed description, replace this with some info about your bot!",
          "thumbnail": "${dbsVars.CommandAuthor.user.defaultAvatarURL}",
          "image": "",
          "footer": "This is a Footer text",
          "timestamp": "BOOL_TRUE@@",
          "savetovariable": "",
          "savetovariabletype": "temp"
        }
      ]
    },
    {
      "name": "info",
      "actions": [
        {
          "name": "show info",
          "category": "Message",
          "type": "Send Message",
          "channelname": "${dbsVars.DefaultChannel.id}",
          "messagetext": "Command Author: ${dbsVars.CommandAuthor.displayName}\nTagged author: <@${dbsVars.CommandAuthor.id}>\nYou can insert more values by clicking the \"Insert Variable\" button next to this field!",
          "savetovariable": "",
          "savetovariabletype": "temp"
        }
      ]
    },
    {
      "name": "helper",
      "actions": [
        {
          "name": "helper menu",
          "category": "Message",
          "type": "Send Embed",
          "channelname": "${dbsVars.CommandChannel.id}",
          "color": "5CFAFF",
          "title": "Helper Menu",
          "url": "",
          "authorname": "Shops Bot",
          "authorimageurl": "",
          "authorlink": "",
          "description": "Hi, <@${dbsVars.CommandAuthor.id}> This is a embed test with nothing else to it at all.",
          "thumbnail": "",
          "image": "",
          "footer": "",
          "timestamp": "false",
          "savetovariable": "",
          "savetovariabletype": "temp"
        }
      ]
    },
    {
      "name": "start-giveaway",
      "actions": [
        {
          "name": "Check Permissions",
          "category": "User Action",
          "type": "Check User Permissions",
          "trueActions": [
            {
              "name": "channel",
              "category": "Variable",
              "type": "Store Value in Variable",
              "varname": "channelname",
              "vartype": "Text",
              "savevartype": "temp",
              "param": "1"
            },
            {
              "name": "checkChannel",
              "category": "Variable",
              "type": "Check Variable Value",
              "trueActions": [
                {
                  "name": "getDuration",
                  "category": "Variable",
                  "type": "Store Value in Variable",
                  "varname": "duration",
                  "vartype": "Text",
                  "savevartype": "temp",
                  "param": "2"
                }
              ],
              "falseActions": [
                {
                  "name": "noChannel",
                  "category": "Message",
                  "type": "Send Message",
                  "channelname": "${dbsVars.CommandAuthor.id}",
                  "messagetext": "You have to mention a valid channel. ",
                  "savetovariable": "",
                  "savetovariabletype": "temp"
                }
              ],
              "varname": "${tempVars.channelname}",
              "savevartype": "temp",
              "compare": "equal to",
              "value": "${dbsVars.CommandChannel.name}"
            }
          ],
          "falseActions": [
            {
              "name": "No permissions",
              "category": "Message",
              "type": "Send Message",
              "channelname": "${dbsVars.CommandChannel.id}",
              "messagetext": "You need to have the manage messages permissions to start giveaways.",
              "savetovariable": "",
              "savetovariabletype": "temp"
            }
          ],
          "user": "${dbsVars.CommandAuthor.id}",
          "permissions": [
            "MANAGE_MESSAGES"
          ]
        }
      ]
    }
  ]
}